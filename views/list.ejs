<%- include('header') -%> 

        <div class="container header">
            <h1><%= listTitle %></h1>
        </div>
        
        <div class="container container-md itemList">
            
            <% items.forEach( item => { %>
            <div class="item row">
                <input type="checkbox" class="col-auto form-check"><p class="col-auto"><%= item %></p>
            </div>
            <% }) %>   
            

            <form class="row g-2" action=<%= list %> method="post">
                <div class="col-sm-10">
                    <input class="form-control" type="text" name="newItem" id="" autocomplete="off">
                </div>
                <div class="col-auto">
                    <button class="btn btn-primary" type="submit">Add Item</button>
                </div>
            </form>


        </div>


<%- include('footer') -%> 